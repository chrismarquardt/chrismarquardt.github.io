          "_sourceTheme": "mobirise5",
          "_anchor": "custom-html-y",
          "_PHPplaceholders": [],
          "_JSplaceholders": [
            "<script>\n\tfunction makeid(length) {\n  \tvar result           = '';\n   var characters       = 'abcdefghijk23456789';\n   var charactersLength = characters.length;\n   for ( var i = 0; i < length; i++ ) {\n      result += characters.charAt(Math.floor(Math.random() * charactersLength));\n   }\n   return result;\n}\nfunction loslos() {\n\tvar streamid=makeid(4);\n    document.getElementById('pushvar').innerHTML = 'Open this link in two separate Chrome browser tabs (or even two separate Chrome browsers on two different computers):<br/> <strong>https://obs.ninja/?room='+streamid+'&wc</strong>';\n//     document.getElementById('pullvar').innerHTML = 'Open this link in another Chrome tab:<br/> <strong>https://obs.ninja/?view='+streamid+'&bitrate=3000</strong>';    \n    var x = document.getElementById(\"hiddeninstructions\");\n    x.style.display = \"block\";\n    var y = document.getElementById(\"clickMe\");\n    y.style.display = \"none\";\n}\n    </script>"
